<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nestedScrollView"
    android:background="@color/white"
    android:padding="16dp"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.google.android.material.textview.MaterialTextView
            android:textSize="18sp"
            android:textColor="@color/black"
            android:id="@+id/dish_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Daing na Bangus"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <com.google.android.material.textview.MaterialTextView
            android:textSize="16sp"
            android:textColor="#b6b7c3"
            android:id="@+id/featured_recipe_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Featured Recipe"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/dish_name"/>
        <com.google.android.material.divider.MaterialDivider
            android:id="@+id/materialDivider"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:layout_marginTop="16dp"
            android:layout_marginBottom="16dp"
            android:layout_marginVertical="16dp"
            app:layout_constraintTop_toBottomOf="@+id/featured_recipe_label"/>
        <com.google.android.material.textview.MaterialTextView
            android:textSize="16sp"
            android:id="@+id/serving_size_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Serving Size (grams)"
            app:layout_constraintBottom_toTopOf="@+id/materialDivider2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/materialDivider"/>
        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/serving_size_input_layout"
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="14dp"
            app:boxCornerRadiusBottomEnd="8dp"
            app:boxCornerRadiusBottomStart="8dp"
            app:boxCornerRadiusTopEnd="8dp"
            app:boxCornerRadiusTopStart="8dp"
            app:boxStrokeColor="@color/green_500"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/materialDivider"
            style="@style/Widget.Material3.TextInputLayout.OutlinedBox">
            <com.google.android.material.textfield.TextInputEditText
                android:textColor="@color/green_500"
                android:id="@+id/serving_size_input"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="100"
                android:inputType="number"/>
        </com.google.android.material.textfield.TextInputLayout>
        <com.google.android.material.divider.MaterialDivider
            android:id="@+id/materialDivider2"
            android:visibility="invisible"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:layout_marginTop="16dp"
            android:layout_marginBottom="16dp"
            android:layout_marginVertical="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/serving_size_input_layout"/>
        <com.google.android.material.textview.MaterialTextView
            android:textSize="16sp"
            android:id="@+id/meal_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Meal"
            android:fontFamily="@font/poppins"
            app:layout_constraintBottom_toTopOf="@+id/materialDivider3"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/materialDivider2"/>
        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/dropdownLayout"
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="14dp"
            android:hint="Breakfast"
            app:boxStrokeColor="@color/green_500"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/materialDivider2"
            style="@style/RoundedTextInputLayoutAutoComplete">
            <com.google.android.material.textfield.MaterialAutoCompleteTextView
                android:textColor="@color/green_500"
                android:id="@+id/dropdownMenu"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="none"
                android:fontFamily="@font/poppins"/>
        </com.google.android.material.textfield.TextInputLayout>
        <com.google.android.material.divider.MaterialDivider
            android:id="@+id/materialDivider3"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:layout_marginTop="16dp"
            android:layout_marginBottom="16dp"
            android:layout_marginVertical="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/dropdownLayout"/>
        <com.google.android.material.card.MaterialCardView
            android:id="@+id/cardNutrientsView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="16dp"
            android:elevation="4dp"
            app:cardBackgroundColor="@android:color/white"
            app:cardCornerRadius="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/materialDivider3"
            app:strokeColor="@android:color/transparent">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:padding="18dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/nutrientsTitle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Daily Goals"
                    android:fontFamily="@font/poppins"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <FrameLayout
                    android:id="@+id/frameLayoutCalories"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    app:layout_constraintBottom_toTopOf="@+id/frameLayoutProteins"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/nutrientsTitle"
                    app:layout_constraintVertical_bias="0.5">
                    <com.google.android.material.progressindicator.CircularProgressIndicator
                        android:id="@+id/circularProgressIndicatorCalories"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:progress="69"
                        app:indicatorColor="@color/green_700"
                        app:indicatorSize="48dp"
                        app:trackThickness="4dp"/>
                    <View
                        android:layout_gravity="center"
                        android:background="@drawable/circle_shape"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:backgroundTint="@color/green_200"/>
                    <TextView
                        android:textSize="12sp"
                        android:textColor="@color/black"
                        android:layout_gravity="center"
                        android:id="@+id/centerTextCalories"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="7.0g"
                        android:fontFamily="@font/poppins_semibold"/>
                </FrameLayout>
                <TextView
                    android:textSize="12sp"
                    android:textColor="@android:color/black"
                    android:id="@+id/caloriesTitle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Calories"
                    app:layout_constraintEnd_toEndOf="@+id/frameLayoutCalories"
                    app:layout_constraintStart_toStartOf="@+id/frameLayoutCalories"
                    app:layout_constraintTop_toBottomOf="@+id/frameLayoutCalories"/>
                <FrameLayout
                    android:id="@+id/frameLayoutCarbs"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="24dp"
                    app:layout_constraintEnd_toStartOf="@+id/frameLayoutProteins"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/frameLayoutCalories"
                    app:layout_constraintVertical_bias="0.5">
                    <com.google.android.material.progressindicator.CircularProgressIndicator
                        android:id="@+id/circularProgressIndicatorCarbs"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:progress="69"
                        app:indicatorColor="@color/blue_700"
                        app:indicatorSize="48dp"
                        app:trackThickness="4dp"/>
                    <View
                        android:layout_gravity="center"
                        android:background="@drawable/circle_shape"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:backgroundTint="@color/blue_500"/>
                    <TextView
                        android:textSize="12sp"
                        android:textColor="@color/black"
                        android:layout_gravity="center"
                        android:id="@+id/centerTextCarbs"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="7.0g"
                        android:fontFamily="@font/poppins_semibold"/>
                </FrameLayout>
                <TextView
                    android:textSize="12sp"
                    android:textColor="@android:color/black"
                    android:id="@+id/carbsTitle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:text="Carbs"
                    app:layout_constraintEnd_toEndOf="@+id/frameLayoutCarbs"
                    app:layout_constraintStart_toStartOf="@+id/frameLayoutCarbs"
                    app:layout_constraintTop_toBottomOf="@+id/frameLayoutCarbs"/>
                <FrameLayout
                    android:id="@+id/frameLayoutProteins"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="24dp"
                    app:layout_constraintEnd_toStartOf="@+id/frameLayoutYellow"
                    app:layout_constraintStart_toEndOf="@+id/frameLayoutCarbs"
                    app:layout_constraintTop_toBottomOf="@+id/frameLayoutCalories"
                    app:layout_constraintVertical_bias="0.5">
                    <com.google.android.material.progressindicator.CircularProgressIndicator
                        android:id="@+id/circularProgressIndicatorProteins"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:progress="100"
                        app:indicatorColor="@color/pink_700"
                        app:indicatorSize="48dp"
                        app:trackThickness="4dp"/>
                    <View
                        android:layout_gravity="center"
                        android:background="@drawable/circle_shape"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:backgroundTint="@color/pink_500"/>
                    <TextView
                        android:textSize="12sp"
                        android:textColor="@color/black"
                        android:layout_gravity="center"
                        android:id="@+id/centerTextProteins"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="6.5g"
                        android:fontFamily="@font/poppins_semibold"/>
                </FrameLayout>
                <TextView
                    android:textSize="12sp"
                    android:textColor="@android:color/black"
                    android:id="@+id/proteinsTitle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:text="Proteins"
                    app:layout_constraintEnd_toEndOf="@+id/frameLayoutProteins"
                    app:layout_constraintStart_toStartOf="@+id/frameLayoutProteins"
                    app:layout_constraintTop_toBottomOf="@+id/frameLayoutProteins"/>
                <FrameLayout
                    android:id="@+id/frameLayoutYellow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="24dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/frameLayoutProteins"
                    app:layout_constraintTop_toBottomOf="@+id/frameLayoutCalories"
                    app:layout_constraintVertical_bias="0.5">
                    <com.google.android.material.progressindicator.CircularProgressIndicator
                        android:id="@+id/circularProgressIndicatorFats"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:progress="80"
                        app:indicatorColor="@color/yellow_700"
                        app:indicatorSize="48dp"
                        app:trackThickness="4dp"/>
                    <View
                        android:layout_gravity="center"
                        android:background="@drawable/circle_shape"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:backgroundTint="@color/yellow_500"/>
                    <TextView
                        android:textSize="12sp"
                        android:textColor="@color/black"
                        android:layout_gravity="center"
                        android:id="@+id/centerTextFats"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="8.0g"
                        android:fontFamily="@font/poppins_semibold"/>
                </FrameLayout>
                <TextView
                    android:textSize="12sp"
                    android:textColor="@android:color/black"
                    android:id="@+id/yellowTitle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:text="Fats"
                    app:layout_constraintEnd_toEndOf="@+id/frameLayoutYellow"
                    app:layout_constraintStart_toStartOf="@+id/frameLayoutYellow"
                    app:layout_constraintTop_toBottomOf="@+id/frameLayoutYellow"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </com.google.android.material.card.MaterialCardView>
        <com.google.android.material.divider.MaterialDivider
            android:id="@+id/materialDivider4"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            app:layout_constraintBottom_toBottomOf="@+id/cardNutrientsView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
        <com.google.android.material.textview.MaterialTextView
            android:textSize="12sp"
            android:id="@+id/nutritionFactsLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="Nutrition Facts"
            android:fontFamily="@font/poppins"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/materialDivider4"/>
        <com.google.android.material.button.MaterialButton
            android:textSize="12sp"
            android:id="@+id/showHidebutton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Show"
            app:icon="@drawable/baseline_keyboard_arrow_down_24"
            app:iconGravity="end"
            app:iconPadding="1dp"
            app:iconSize="12sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/materialDivider4"
            style="@style/Widget.Material3.Button.TextButton.Icon"/>
        <com.google.android.material.card.MaterialCardView
            android:id="@+id/nutritionFactsCard"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:layout_marginBottom="16dp"
            app:cardBackgroundColor="@color/white"
            app:cardCornerRadius="12dp"
            app:cardElevation="4dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/showHidebutton">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:padding="16dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <com.google.android.material.textview.MaterialTextView
                    android:textSize="16sp"
                    android:textColor="@color/text_black"
                    android:id="@+id/nutritionFactsTitle"
                    android:padding="4dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="12dp"
                    android:text="Nutrition Facts"
                    android:fontFamily="@font/poppins_semibold"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <com.google.android.material.divider.MaterialDivider
                    android:background="@color/gray_200"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_marginTop="4dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/nutritionFactsTitle"/>
                <com.google.android.material.textview.MaterialTextView
                    android:textSize="14sp"
                    android:textColor="@color/text_black"
                    android:id="@+id/caloriesLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:text="Calories"
                    android:fontFamily="@font/poppins_semibold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/nutritionFactsTitle"/>
                <com.google.android.material.textview.MaterialTextView
                    android:textSize="14sp"
                    android:textColor="@color/text_black"
                    android:id="@+id/caloriesValue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="120"
                    android:fontFamily="@font/poppins_semibold"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/caloriesLabel"/>
                <com.google.android.material.divider.MaterialDivider
                    android:background="@color/gray_200"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_marginTop="4dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/caloriesLabel"/>
                <com.google.android.material.textview.MaterialTextView
                    android:textSize="14sp"
                    android:textStyle="bold"
                    android:id="@+id/carbsLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12dp"
                    android:text="Carbohydrates"
                    android:fontFamily="@font/poppins_semibold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/caloriesLabel"/>
                <com.google.android.material.textview.MaterialTextView
                    android:textSize="14sp"
                    android:textColor="@color/text_black"
                    android:id="@+id/carbsValue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="6.5g"
                    android:fontFamily="@font/poppins_semibold"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/carbsLabel"/>
                <com.google.android.material.textview.MaterialTextView
                    android:textSize="14sp"
                    android:textColor="@color/text_black"
                    android:id="@+id/fibersLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp"
                    android:text="Fibers"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/carbsLabel"/>
                <com.google.android.material.textview.MaterialTextView
                    android:textSize="14sp"
                    android:textColor="@color/text_black"
                    android:id="@+id/fibersValue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="2.3g"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/fibersLabel"/>
                <com.google.android.material.textview.MaterialTextView
                    android:textSize="14sp"
                    android:textColor="@color/text_black"
                    android:id="@+id/sugarsLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp"
                    android:text="Sugars"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/fibersLabel"/>
                <com.google.android.material.textview.MaterialTextView
                    android:textSize="14sp"
                    android:textColor="@color/text_black"
                    android:id="@+id/sugarsValue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0g"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/sugarsLabel"/>
                <com.google.android.material.divider.MaterialDivider
                    android:background="@color/gray_200"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/sugarsLabel"/>
                <com.google.android.material.textview.MaterialTextView
                    android:textSize="14sp"
                    android:textColor="@color/text_black"
                    android:id="@+id/proteinsLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12dp"
                    android:text="Proteins"
                    android:fontFamily="@font/poppins_semibold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/sugarsLabel"/>
                <com.google.android.material.textview.MaterialTextView
                    android:textSize="14sp"
                    android:textColor="@color/text_black"
                    android:id="@+id/proteinsValue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="21.3g"
                    android:fontFamily="@font/poppins_semibold"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/proteinsLabel"/>
                <com.google.android.material.divider.MaterialDivider
                    android:background="@color/gray_200"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/proteinsLabel"/>
                <com.google.android.material.textview.MaterialTextView
                    android:textSize="14sp"
                    android:textStyle="bold"
                    android:textColor="@color/text_black"
                    android:id="@+id/fatsLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12dp"
                    android:text="Fats"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/proteinsLabel"/>
                <com.google.android.material.textview.MaterialTextView
                    android:textSize="14sp"
                    android:textColor="@color/text_black"
                    android:id="@+id/fatsValue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="6.5g"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/fatsLabel"/>
                <com.google.android.material.textview.MaterialTextView
                    android:textSize="14sp"
                    android:textColor="@color/text_black"
                    android:id="@+id/saturatedFatsLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp"
                    android:text="Saturated fatty acids"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/fatsLabel"/>
                <com.google.android.material.textview.MaterialTextView
                    android:textSize="14sp"
                    android:textColor="@color/text_black"
                    android:id="@+id/saturatedFatsValue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="2.3g"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/saturatedFatsLabel"/>
                <com.google.android.material.textview.MaterialTextView
                    android:textSize="14sp"
                    android:textColor="@color/text_black"
                    android:id="@+id/monounsaturatedFatsLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp"
                    android:text="Monounsaturated fatty acid"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/saturatedFatsLabel"/>
                <com.google.android.material.textview.MaterialTextView
                    android:textSize="14sp"
                    android:textColor="@color/text_black"
                    android:id="@+id/monounsaturatedFatsValue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="2.3g"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/monounsaturatedFatsLabel"/>
                <com.google.android.material.divider.MaterialDivider
                    android:background="@color/gray_200"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    app:layout_constraintEnd_toEndOf="parent"
